			</HorizontalLayout>
			<HorizontalLayout height="24"  padding="0,0,0,10"  name="usernameLayout" >
				<Label text="@str:IDS_Profile_WechatId:1150" padding="0,1,0,0" width="60" height="22" font="2" align="left" textcolor="#FF999999"/>
				<EmotionText name="profileUsernameLab" text="" textpadding="0,0,5,0" padding="30,0,0,0" width="270" height="20" font="2" textcolor="#FF000000"/>
			</HorizontalLayout>
		</VerticalLayout>

		<VerticalLayout width="270" maxheight="240" visible="false" padding="0,0,0,0" name="detailLayout" vscrollbar="true">
				<Label name="subTitle" text="@str:IDS_Enterprise_Info:4145" textcolor="FF999999" font="2" padding="0,0,0,10"/>
		</VerticalLayout>

		<VerticalLayout name="replyInfoLayout" width="370" maxheight="150" padding="0,32,0,0,"  borderround="5,5"   vscrollbar="true">
