		</HorizontalLayout>
		<Button name="fmessageProfileSendBtn" text="@str:IDS_Profile_SendMsg:1152" padding="91,2,0,0" width="168" height="37" textcolor="#FFFFFFFF" font="2" valign="top" textpadding="0,-5,0,0" visible="false" 
			normalimage="file='common\Common_Btn_ok_normal.png' corner='5,10,5,10'" hotimage="file='common\Common_Btn_ok_hover.png' corner='5,10,5,10'" pushedimage="file='common\Common_Btn_ok_click.png' corner='5,10,5,10'" />
