				</HorizontalLayout>
				<EmotionText name="profileSignatureLab" text="" padding="0,4,0,0" width="300" height="0" font="2" align="left" textcolor="#FF999999" visible="false" minwidth="25" minheight="15" maxwidth= "300" />
			</VerticalLayout>
			<HeadImgUI name="profileHeadBtn"  width="60" height="60" recycle="false" />
		</HorizontalLayout>
		
		<VerticalLayout name="profileInfoLayout"  height="30" width="370" padding="0,32,0,0" bordersize="0,0,0,1" bordercolor="#FFE7E7E7">
			<HorizontalLayout height="24" padding="0,0,0,10" name="regionLayout">
